@page
@model NewRsaKey
@{
    //ViewData["Title"] = "Generate new Rsa key";
    ViewData["ActivePage"] = ManageNavPages.NewRsaKey;
}
<h3>Generate new RSA Key</h3>
<h4>@ViewData["Title"]</h4>
<partial name="_StatusMessage" for="StatusMessage" />
<div class="row">
    <div class="col-md-6">
        <form id="change-password-form" method="post">
            <div asp-validation-summary="All" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Input.KeyPairName"></label>
                <input asp-for="Input.KeyPairName" class="form-control" />
                <span asp-validation-for="Input.KeyPairName" class="text-danger"></span>
            </div>

            <button type="submit" class="btn btn-primary">Add key pair</button>
        </form>
    </div>
</div>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}